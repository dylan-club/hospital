<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.nicklaus.dao.RegLevelMapper">
    <select id="findPriceById" parameterType="int" resultType="regLevel">
        select * from reglevel where levelId = #{id}
    </select>

    <select id="findAll" resultType="regLevel">
        select * from reglevel;
    </select>
</mapper>