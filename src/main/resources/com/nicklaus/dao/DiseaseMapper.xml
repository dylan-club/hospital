<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.nicklaus.dao.DiseaseMapper">
    <select id="findByDeptId" parameterType="int" resultType="disease">
        select * from disease where deptId = #{deptId}
    </select>
</mapper>